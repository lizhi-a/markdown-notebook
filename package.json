    "@ant-design/pro-layout": "^6.5.0",
    "ahooks": "^3.7.0",
    "antd": "^4.22.4",
    "art-template": "^4.13.2",
    "axios": "^0.27.2",
    "blueimp-md5": "^2.18.0",
    "body-parser": "^1.19.0",
    "classnames": "^2.3.1",
    "express": "^4.17.1",
    "express-art-template": "^1.0.1",
    "express-session": "^1.17.1",
    "highlight.js": "^11.6.0",
    "jquery": "^3.5.1",
    "marked": "^4.0.18",
    "moment": "^2.29.4",
    "mongoose": "^5.11.16",
    "nodemon": "^2.0.7",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.8.1",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.32"
  },
  "devDependencies": {
    "@types/marked": "^4.0.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.32",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
